int main() {
    // Test while loop
    int count = 0;
    while (count < 3) {
        printf(count);
        count = count + 1;
    }
    
    // Test for loop
    int sum = 0;
    for (int i = 1; i < 6; i = i + 1) {
        sum = sum + i;
    }
    printf(sum);
    
    // Test nested loops
    for (int x = 0; x < 2; x = x + 1) {
        int y = 0;
        while (y < 2) {
            printf(x * 100 + y * 10);
            y = y + 1;
        }
    }
    
    return 0;
}
